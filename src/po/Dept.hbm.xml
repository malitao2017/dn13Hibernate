<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="po.Dept" table="t_dept">
		<id name="id" type="integer" column="t_id">
			<generator class="native">
				<param name="sequence">dept_seq</param>
			</generator>
		</id>
		<property name="name" type="string" column="t_name"></property>
		<property name="location" type="string" column="t_loc"></property>
	</class>
</hibernate-mapping>